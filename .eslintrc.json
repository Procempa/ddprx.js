{
    "env": {
        "browser": true,
        "es6": true
    },
    "globals": {
        "_": false,
        "process": false
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            1,
            "single"
        ],
        "semi": [
            2
            /*"never"*/
        ],
        "no-unused-vars": [
            "warn"
        ],
        "no-console": [
            0
        ]
    }
}
