{
  "name": "ddprx.js",
  "version": "0.0.1",
  "description": "Meteor DDP connection using RxJS pattern",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/tape/bin/tape -r babel-register ./test/*.test.js",
    "build": "./node_modules/babel-cli/bin/babel.js src  -s --out-dir build",
    "watch": "./node_modules/babel-cli/bin/babel.js src -w -s --out-dir build",
    "document": "jsdoc ./source -r -d docs",
    "prepublish": "npm run build; npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silveirado/ddprx.js.git"
  },
  "keywords": [
    "DDP",
    "Meteor",
    "RxJS"
  ],
  "author": "Derotino Silveira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silveirado/ddprx.js/issues"
  },
  "homepage": "https://github.com/silveirado/ddprx.js#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-stage-1": "^6.16.0",
    "jsdoc-babel": "^0.3.0",
    "source-map-support": "^0.4.6",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6",
    "tape-promise": "^2.0.1"
  },
  "dependencies": {
    "ajv": "^4.9.2",
    "babel-register": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "ejson": "^2.1.2",
    "lodash": "^4.17.2",
    "rx": "^4.1.0",
    "sinon": "^1.17.6",
    "sockjs-client": "^1.1.1",
    "validate.js": "^0.11.1"
  },
  "babel": {
    "presets": [
		"es2015",
		"stage-1"
	  ],
	  "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "transform-runtime"
    ]
  }
}
